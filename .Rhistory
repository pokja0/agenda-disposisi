# --- 1. Memuat Data ---
# Membaca file CSV
data_agenda <- data_agenda_disposisi
# Membersihkan nama kolom (menghapus spasi dan baris baru yang tidak perlu)
setnames(data_agenda, gsub("[^[:alnum:]]+", "_", names(data_agenda)))
setnames(data_agenda, tolower(names(data_agenda)))
# Menghapus underscore di awal/akhir nama kolom
data_agenda <- fsetex(data_agenda, fun = trimws, patterns = "_", position = "both")
# Konversi kolom 'tanggal_mulai' dan 'tanggal_selesai' menjadi tipe datetime
# 'anytime' dapat menangani berbagai format, termasuk "YYYY-MM-DD HH:MM:SS"
data_agenda <- ft(data_agenda,
TANGGAL_MULAI = fas.POSIXct(TANGGAL_MULAI),
TANGGAL_SELESAI = fas.POSIXct(TANGGAL_SELESAI)
)
# Mengonversi format datetime menjadi string ISO 8601 (YYYY-MM-DDTHH:MM:SS)
# Contoh: 2025-12-14T09:00:00
data_agenda <- ft(data_agenda,
start_time = format(TANGGAL_MULAI, "%Y-%m-%dT%H:%M:%S"),
end_time = format(TANGGAL_SELESAI, "%Y-%m-%dT%H:%M:%S")
)
# Pastikan Anda telah menginstal dan memuat package yang diperlukan
# install.packages(c("tidyverse", "anytime", "janitor"))
library(tidyverse)
library(anytime) # Digunakan untuk konversi tanggal/waktu
library(janitor) # Digunakan untuk membersihkan nama kolom
